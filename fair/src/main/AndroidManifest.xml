<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.paulpaulzhang.fairfest">

    <application
        android:name="cn.paulpaulzhang.fairfest.FairApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="cn.paulpaulzhang.fair.sc.launcher.LauncherActivity">

        </activity>
        <activity android:name="cn.paulpaulzhang.fair.sc.launcher.LauncherScrollActivity">

        </activity>
        <activity
            android:name="cn.paulpaulzhang.fair.sc.sign.SignUpActivity"
            android:label="@string/sign_up">

        </activity>
        <activity
            android:name="cn.paulpaulzhang.fair.sc.sign.SignInActivity"
            android:label="@string/sign_in" />
        <activity android:name="cn.paulpaulzhang.fair.sc.main.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>